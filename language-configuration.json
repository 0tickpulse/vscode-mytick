{
    "comments": {
        "lineComment": "#"
    },
    "brackets": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "<",
            ">"
        ]
    ],
    "autoClosingPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "<",
            ">"
        ]
    ],
    "surroundingPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "<",
            ">"
        ],
        [
            "\"",
            "\""
        ],
        [
            "'",
            "'"
        ]
    ],
    "folding": {
        "markers": {
            "start": "^\\s*#region\\b",
            "end": "^\\s*#endregion\\b"
        }
    },
    "indentationRules": {
        // Matches "asdf:" and "a=["
        "increaseIndentPattern": "^.*(\\:|=\\[)$",
        "decreaseIndentPattern": "^\\s+\\}$"
    },
    "wordPattern": "(^.?[^\\s]+)+|([^\\s\n={[][\\w\\-\\.\/$%&*:\"']+)"
}